    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-helmet": "^5.2.0",
    "react-js-pagination": "^3.0.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-toastify": "^4.0.0-rc.5",
    "redux": "^3.7.2",
